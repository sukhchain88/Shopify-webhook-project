{
  "name": "shopify-webhook-project",
  "version": "1.0.0",
  "description": "A comprehensive Shopify webhook handler project with product management",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:production": "npm run prebuild && tsc --noEmitOnError",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run build -- --watch\" \"node dist/index.js\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "shopify",
    "webhook",
    "nodejs",
    "express",
    "typescript",
    "postgresql",
    "sequelize"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "bullmq": "^5.53.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "redis": "^5.5.6",
    "rimraf": "^4.1.4",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.0",
    "@types/pg": "^8.11.10",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.7.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=20.9.0 <21.0.0",
    "npm": ">=10.0.0"
  }
}
