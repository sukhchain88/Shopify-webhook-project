{
  "name": "shopify-webhook-project",
  "version": "1.0.0",
  "description": "A comprehensive Shopify webhook handler project with product management",

  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "build:production": "npm run prebuild && tsc --noEmitOnError && echo 'Build completed successfully'",
    "postbuild": "echo 'TypeScript compilation completed'",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run build -- --watch\" \"nodemon dist/index.js\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:api": "node tests/api-tests.js",
    "typecheck": "tsc --noEmit",
    "render:build": "echo 'Starting Render build...' && npm ci && npm run typecheck && npm run build:production"
  },
  "keywords": [
    "shopify",
    "webhook",
    "nodejs",
    "express",
    "typescript",
    "postgresql",
    "sequelize"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "bullmq": "^5.53.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "redis": "^5.5.6",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8",
    "typescript": "^5.4.5",
    "rimraf": "^6.0.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.15.1",
    "@types/pg": "^8.11.6",
    "@types/redis": "^4.0.10"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=20.15.1 <22.0.0",
    "npm": ">=8.0.0"
  }
}
