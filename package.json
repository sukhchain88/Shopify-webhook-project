{
  "name": "shopify-webhook-project",
  "version": "1.0.0",
  "description": "A comprehensive Shopify webhook handler project with product management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "nodemon dist/index.js",
    "dev": "concurrently \"npm run build -- --watch\" \"nodemon dist/index.js\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:api": "node tests/api-tests.js",
    "serve": "concurrently \"npm run build -- --watch\" \"nodemon dist/index.js\"",
    "watch": "tsc -w",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "remove-error": "node scripts/remove-error-column.js",
    "fix-db": "node scripts/fix-all-tables.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "shopify",
    "webhook",
    "nodejs",
    "express",
    "typescript",
    "postgresql",
    "sequelize"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "sequelize": "^6.37.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
